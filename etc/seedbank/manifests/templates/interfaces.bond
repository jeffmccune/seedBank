# network config generated by seedbank and Puppet

auto lo
iface lo inet loopback

auto bond0
iface bond0 inet static
  address <%= ip %>
  netmask <%= netmask %>
  broadcast <%= broadcast %>
  gateway <%= gateway %>
  slaves eth0 eth1
  bond_mode active-backup
  bond_arp_interval 200
  bond_arp_ip_target <%= gateway %>
