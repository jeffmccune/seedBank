bogus-priv

# only bind to interface eth1 
interface=eth1

# enable /etc/hosts based DNS server
expand-hosts

# domain to use for DNS
domain=seedbank.virtual

# DHCP server
dhcp-range=192.168.20.100,192.168.20.150,1h

# Enable PXE booting
dhcp-boot=pxelinux.0,slug,192.168.20.1

# TFTP server
enable-tftp
tftp-root=/var/lib/tftpboot
